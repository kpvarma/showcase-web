{
  "name": "bookmark-app",
  "version": "1.0.0",
  "description": "A simple bookmark app on google extension",
  "main": "src/index.js",
  "scripts": {
    "watch-content": "nodemon --watch data --ext mdx --exec \"npx contentlayer2 build\"",
    "start": "webpack serve --mode development --open --host 0.0.0.0 --port 8080",
    "start-prod": "webpack serve --open",
    "build": "npx contentlayer2 build && webpack --mode production",
    "build-no-cache": "rm -rf .contentlayer && npx contentlayer2 build && webpack --mode production",
    "postbuild": "node generate-static-html.js"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@fontsource/roboto": "^5.0.8",
    "@formspree/react": "^2.5.1",
    "@mdx-js/esbuild": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@mui/icons-material": "^6.1.0",
    "@mui/material": "^6.1.9",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@react-pdf/renderer": "^4.1.5",
    "axios": "^0.24.0",
    "chart.js": "^4.4.7",
    "contentlayer2": "^0.5.3",
    "file-saver": "^2.0.5",
    "highlight.js": "^11.11.0",
    "jszip": "^3.10.1",
    "markdown-to-jsx": "^7.x.x",
    "pdfjs-dist": "4.0.379",
    "pliny": "^0.4.0",
    "react": "^18.0.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-material-ui-carousel": "^3.4.2",
    "react-router-dom": "^6.22.0",
    "react-scripts": "^5.0.1",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-highlight": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.0.0",
    "chokidar-cli": "^3.0.0",
    "contentlayer": "^0.3.4",
    "css-loader": "^6.10.0",
    "html-webpack-plugin": "^5.6.0",
    "nodemon": "^3.1.7",
    "style-loader": "^3.3.4",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=22.11.0"
  },
  "author": "Krishna Prasad Varma",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
